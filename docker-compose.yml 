# docker-compose.yml
version: '3.8'

services:
  service-nestjs-assessment-test:
    build: .
    container_name: service-nestjs-assessment-test
    restart: unless-stopped
    ports:
      - "3003:3003"
    env_file:
      - .env
    networks:
      - ngx-net

networks:
  ngx-net:
    external: true